{
    "manifest_version": 3,
    "name": "Trip time",
    "version": "1.0",
    "description": "Calculate the distance between two points",
    "icons": { "48": "data/logo.png" },
    "action": {
      "default_popup": "popup.html",
      "default_icon": "data/logo.png"
    },
    "permissions": [
        "activeTab",
        "storage"
    ],
    "host_permissions": [
        "https://jbccc.pythonanywhere.com/*"
    ],
    "commands": {
        "_execute_action": {
          "suggested_key": {
            "default": "Ctrl+B",
            "mac": "Command+B"
          },
          "description": "Opens the popup UI"
        }
      }
}